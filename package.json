{
  "name": "aws-s3-bucket",
  "version": "1.0.0",
  "description": "Use to upload and retrieve infomation from S3 bucket",
  "main": "index.js",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon index.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
